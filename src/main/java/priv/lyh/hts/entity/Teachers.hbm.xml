<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="priv.lyh.hts.entity.Teachers" table="teachers" schema="hts_db">
        <id name="teacNum" column="teac_num"/>
        <property name="teacName" column="teac_name"/>
        <property name="teacClass" column="teac_class"/>
        <property name="teacPwd" column="teac_pwd"/>
    </class>
</hibernate-mapping>