<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="priv.lyh.hts.entity.Students" table="students" schema="hts_db">
        <id name="stuNum" column="stu_num" />
        <property name="stuName" column="stu_name"/>
        <property name="stuInstitute" column="stu_institute"/>
        <property name="stuSex" column="stu_sex"/>
        <property name="stuGrade" column="stu_grade"/>
        <property name="stuMajor" column="stu_major"/>
        <property name="stuClass" column="stu_class"/>
        <property name="password" column="stu_pwd"/>
    </class>
</hibernate-mapping>